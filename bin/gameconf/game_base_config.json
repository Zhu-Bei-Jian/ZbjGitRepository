	"SkillConf":[
		{ "SkillID": 1, "Name": "断肠", "Desc": "翻牌技：沉默1个武将。若此武将在交战区，沉默2个武将。", "SkillType": 1, "SelectCardCount": 1, "GainAreaType": 2, "Buffs":[ 1 ] },
		{ "SkillID": 2, "Name": "殿后", "Desc": "翻牌技：选择一名敌方武将，双方同时受到X点伤害。（X为生命值较低武将的生命值）", "SkillType": 1, "SelectCardCount": 1 },
		{ "SkillID": 3, "Name": "奸雄", "Desc": "被动技：该武将受到伤害但未阵亡时，全体友方武将+1攻击力。", "SkillType": 2, "Buffs":[ 2 ] },
		{ "SkillID": 4, "Name": "智迟", "Desc": "翻牌技：使一名其他武将获得此效果：每次主动攻击后恢复4点生命值。", "SkillType": 1, "SelectCardCount": 1, "Buffs":[ 3 ] },
		{ "SkillID": 5, "Name": "国色", "Desc": "翻牌技：使一名其他友方武将本回合无攻击次数限制。", "SkillType": 1, "SelectCardCount": 1, "Buffs":[ 4 ] },
		{ "SkillID": 6, "Name": "急袭", "Desc": "翻牌技：交换任意两名武将的位置。", "SkillType": 1, "SelectCardCount": 2 },
		{ "SkillID": 7, "Name": "强袭", "Desc": "被动技：此武将无法攻击敌方军营。", "SkillType": 2 },
		{ "SkillID": 8, "Name": "离间", "Desc": "翻牌技：选择1名其他友方武将攻击1名敌方武将直至一方阵亡。", "SkillType": 1, "SelectCardCount": 2 },
		{ "SkillID": 9, "Name": "重装", "Desc": "重装：只能正面放置；上场、攻击、移动均需要花费2点行动点。", "SkillType": 2 },
		{ "SkillID": 10, "Name": "眩惑", "Desc": "翻牌技：使一名其他武将的攻击力和生命值互换。", "SkillType": 1, "SelectCardCount": 1, "Buffs":[ 5 ] },
		{ "SkillID": 11, "Name": "破袭", "Desc": "翻牌技：对一名其他武将造成8点伤害。", "SkillType": 1, "SelectCardCount": 1, "Value1": 8 },
		{ "SkillID": 12, "Name": "陷阵", "Desc": "翻牌技：对交战区所有武将造成4点伤害。", "SkillType": 1, "Value1": 4 },
		{ "SkillID": 13, "Name": "武圣", "Desc": "翻牌技：此武将获得自身攻击力的生命值。", "SkillType": 1, "Buffs":[ 6 ] },
		{ "SkillID": 14, "Name": "天妒", "Desc": "被动技：你的回合结束时，此武将受到5点伤害。", "SkillType": 2 },
		{ "SkillID": 15, "Name": "解烦", "Desc": "翻牌技：使友方军营回复8点生命或对敌方军营造成8点伤害。", "SkillType": 1, "SelectCardCount": 1, "Value1": 8 },
		{ "SkillID": 16, "Name": "鸩毒", "Desc": "翻牌技：对友方一名其他明牌武将造成3点伤害。", "SkillType": 1, "SelectCardCount": 1, "Value1": 3 },
		{ "SkillID": 17, "Name": "青囊", "Desc": "翻牌技：使一名武将恢复10点生命值。", "SkillType": 1, "SelectCardCount": 1, "Value1": 10 },
